<?xml version="1.0" encoding="UTF-8"?>
<module type="JAVA_MODULE" version="4">
  <!-- Le composant racine du module -->
  <component name="NewModuleRootManager">
    <!-- Définit le répertoire de sortie pour les classes compilées -->
    <output url="file://$MODULE_DIR$/bin" />
    <!-- Exclut le répertoire de sortie d'être inclus comme source -->
    <exclude-output />
    <!-- Spécifie le répertoire racine de contenu pour le module -->
    <content url="file://$MODULE_DIR$" />
    <!-- Définit un dossier source pour le module, qui n'est pas destiné aux tests -->
    <orderEntry type="sourceFolder" forTests="false" />
  </component>
</module>
